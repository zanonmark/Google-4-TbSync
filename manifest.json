{
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "version": "0.6.0-webext",
    "author": "Marco Zanon",
    "default_locale": "en-US",
    "applications": {
        "gecko": {
            "id": "google-4-tbsync@marcozanon.com",
            "strict_min_version": "102.3.0",
            "strict_max_version": "102.*"
        }
    },
    "icons": {
        "16": "skin/icon16.png",
        "32": "skin/icon32.png",
        "64": "skin/icon64.png"
    },
    "background": {
        "scripts": [
            "content/includes/AddressBookEventManager.js",
            "background.js"
        ]
    },
    "permissions": [
        "addressBooks"
    ],
    "experiment_apis": {
        "BootstrapLoader": {
            "schema": "content/api/BootstrapLoader/schema.json",
            "parent": {
                "scopes": [
                    "addon_parent"
                ],
                "paths": [
                    [
                        "BootstrapLoader"
                    ]
                ],
                "script": "content/api/BootstrapLoader/implementation.js"
            }
        }
    }
}
